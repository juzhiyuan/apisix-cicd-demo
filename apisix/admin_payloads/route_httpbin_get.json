{
  "name": "httpbin_get",
  "desc": "Proxy to httpbin GET",
  "service_id": "httpbin_service",
  "methods": ["GET"],
  "uris": [
    "/get"
  ],
  "plugins": {
    "cors": {},
    "limit-count": {
      "count": 3,
      "time_window": 30,
      "rejected_code": 429,
      "key": "remote_addr",
      "key_type": "var",
      "policy": "local"
    }
  }
}
